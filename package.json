{
  "name": "json-import-loader",
  "version": "2.0.0",
  "description": "Allows import files into a json file, contains webpack-loader and node API.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prepublishOnly": "npm-run-all -s validate build",
    "validate": "npm-run-all -p lint test",
    "dev": "npm-run-all -p dev:*",
    "dev:babel": "babel ./src -x \".ts\" --out-dir ./ --watch",
    "dev:ts": "tsc --noEmit --allowJs --watch",
    "build": "npm-run-all -s clean build:*",
    "build:babel": "babel ./src -x \".ts\" -x \".js\" --out-dir ./",
    "build:ts": "tsc -p ./tsconfig.build.json && shx cp -Rf decl/* . && shx rm -rf decl",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "clean": "npm-run-all clean:*",
    "clean:test": "shx rm -rf coverage .nyc_output",
    "clean:npm": "shx rm -rf lib decl index.js",
    "doc": "npm-run-all -p doc:*",
    "doc:typedoc": "typedoc --out docs/ src/ --mode file",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json -t verbose",
    "fix": "npm-run fix:ts prettify",
    "fix:ts": "tslint -c tslint.json -p tsconfig.json --fix",
    "prettify": "prettier --write \"**/*.{js,ts,json}\"",
    "check-types": "tsc --noEmit",
    "precommit": "lint-staged"
  },
  "author": "Arjan van Wijk <thanarie@gmail.com> (ThaNarie)",
  "homepage": "https://www.thanarie.nl/",
  "license": "MIT",
  "keywords": [
    "webpack",
    "loader",
    "json",
    "yaml",
    "include",
    "import",
    "partial"
  ],
  "bugs": {
    "url": "https://github.com/ThaNarie/json-import-loader/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThaNarie/json-import-loader.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-stage-3": "^7.8.3",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/jest": "^24.9.1",
    "@types/webpack": "^4.41.21",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^9.5.0",
    "memory-fs": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "require-from-string": "latest",
    "shx": "^0.3.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "yaml-loader": "^0.6.0"
  },
  "dependencies": {
    "loader-utils": "^1.2.3"
  }
}
